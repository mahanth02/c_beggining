#include <stdio.h>

int main() {
    int a, i, result;

    printf("Enter the number for the table: ");
    scanf("%d", &a);

    for (i = 0; i <= 10; i++) {  // Removed the incorrect semicolon
        result = a * i;
        printf("%d x %d = %d\n", a, i, result);  // Corrected format
    }

    return 0;







//program 2

int main() {
    int marks;  // Missing semicolon added

    printf("Enter marks (0 to 100): ");
    scanf("%d", &marks);

    if (marks >= 90 && marks <= 100) {
        printf("Grade A\n");
    }
    else if (marks >= 80 && marks < 90) {
        printf("Grade B\n");
    }
    else if (marks >= 70 && marks < 80) {
        printf("Grade C\n");
    }
    else if (marks >= 60 && marks < 70) {
        printf("Grade D\n");
    }
    else if (marks >= 0 && marks < 60) {
        printf("Fail\n");
    }
    else {
        printf("Invalid input! Marks should be between 0 and 100.\n");
    }

    return 0;
}}
